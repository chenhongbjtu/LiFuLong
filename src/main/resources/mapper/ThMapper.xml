<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.chef.assist.mapper.ThMapper">

    <!--根据产品名称查询温湿度-->
    <select id="findThByName" parameterType="java.lang.String" resultType="com.chef.assist.model.Th">
        select * from th
        where name = #{name}
    </select>



</mapper>
